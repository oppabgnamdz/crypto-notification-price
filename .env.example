# Token của bot Telegram (lấy từ BotFather)
BOT_TOKEN=your_telegram_bot_token

# Đường dẫn kết nối MongoDB từ xa (MongoDB Atlas hoặc dịch vụ khác)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/telegram-price-bot

# Token GitHub Personal Access Token (cần quyền gist)
GITHUB_TOKEN=ghp_your_github_personal_access_token

# ID của Gist để lưu dữ liệu
GIST_ID=your_gist_id

# API key cho CoinGecko (để tăng giới hạn API)
COINGECKO_API_KEY=your_coingecko_api_key 